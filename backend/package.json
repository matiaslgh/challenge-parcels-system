{
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec 'kill-port --port 9229 && node --inspect=0.0.0.0:9229 -r ts-node/register' src/index.ts | pino-pretty",
    "start": "node dist/index.js",
    "migrate": ".",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix && prettier --write .",
    "set:integration:up": "docker-compose -f ../docker-compose.test.yml -p parcels_test up -d",
    "set:integration:down": "docker-compose -f ../docker-compose.test.yml -p parcels_test down",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "jest src",
    "test:integration": "yarn set:integration:up && jest integration-tests --runInBand && yarn set:integration:down",
    "watch": "yarn set:integration:up && jest --runInBand --watch && yarn set:integration:down",
    "watch:unit": "jest src --watch",
    "watch:integration": "yarn set:integration:up && jest integration-tests --runInBand --watch && yarn set:integration:down"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-router": "^0.1.15",
    "http-status-codes": "^2.2.0",
    "joi": "^17.9.2",
    "knex": "^2.4.2",
    "pg": "^8.10.0",
    "pg-error-enum": "^0.6.0",
    "pino": "^8.14.1",
    "pino-http": "^8.3.3",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "kill-port": "^2.0.1",
    "nodemon": "^2.0.22",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1"
  }
}
